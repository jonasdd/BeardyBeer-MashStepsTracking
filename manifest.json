{
  "name": "Mash Temp Link",
  "version": "0.1",
  "manifest_version": 2,
  "minimum_chrome_version": "23",
  "description": "Track and Countdown Mashsteps",

  "app": {
    "background": {
      "scripts": [ "background.js" ]
    }
  },

  "icons": {
    "16": "icon.png",
    "128": "icon.png"
  },

  "permissions": [
    "serial",
    "fullscreen",
    "notifications"
  ]
}
